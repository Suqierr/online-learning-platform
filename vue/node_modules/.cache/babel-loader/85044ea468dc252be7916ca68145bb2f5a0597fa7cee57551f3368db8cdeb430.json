{"ast":null,"code":"export default {\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      carouselData: [require('@/assets/imgs/lun-1.jpg'), require('@/assets/imgs/lun-2.jpg'), require('@/assets/imgs/lun-3.jpg'), require('@/assets/imgs/lun-4.jpg')],\n      type: 'VIDEO',\n      recommend: {},\n      rightData: []\n    };\n  },\n  mounted() {\n    this.getData();\n    this.getInformation();\n    this.getSign();\n  },\n  methods: {\n    initValue(type) {\n      this.type = type;\n      this.getData();\n      this.getInformation();\n    },\n    getData() {\n      if ('VIDEO' === this.type) {\n        // 1. 获取推荐的那个积分课程\n        this.$request.get('/course/getRecommend?type=VIDEO').then(res => {\n          if (res.code === '200') {\n            this.recommend = res.data;\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n        // 2. 获取推荐之外的最新的8个课程\n        this.$request.get('/course/selectTop8?type=VIDEO').then(res => {\n          if (res.code === '200') {\n            this.rightData = res.data;\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n      }\n      if ('TEXT' === this.type) {\n        // 1. 获取推荐的那个积分课程\n        this.$request.get('/course/getRecommend?type=TEXT').then(res => {\n          if (res.code === '200') {\n            this.recommend = res.data;\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n        // 2. 获取推荐之外的最新的8个课程\n        this.$request.get('/course/selectTop8?type=TEXT').then(res => {\n          if (res.code === '200') {\n            this.rightData = res.data;\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n      }\n    },\n    getInformation() {\n      // 1. 获取推荐的那个资料\n      this.$request.get('/information/getRecommend').then(res => {\n        if (res.code === '200') {\n          this.fileRecommend = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n      // 2. 获取非推荐的8个资料\n      this.$request.get('/information/selectTop8').then(res => {\n        if (res.code === '200') {\n          this.leftData = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    navTo(id) {\n      // 往课程详情页跳\n      location.href = '/front/courseDetail?id=' + id;\n    },\n    navToInformation(id) {\n      location.href = '/front/informationDetail?id=' + id;\n    },\n    getSign() {\n      this.$request.get('/signin/selectByUserId?id=' + this.user.id).then(res => {\n        if (res.code === '200') {\n          // 渲染数据到页面\n          this.signin = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    signin() {\n      let data = {\n        userId: this.user.id\n      };\n      this.$request.post('/signin/add', data).then(res => {\n        if (res.code === '200') {\n          this.getSign();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","carouselData","require","type","recommend","rightData","mounted","getData","getInformation","getSign","methods","initValue","$request","get","then","res","code","$message","error","msg","fileRecommend","leftData","navTo","id","location","href","navToInformation","signin","userId","post"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"display: flex\">\r\n      <div style=\"flex: 12%; height: 350px; background-color: #0c0c0c\"></div>\r\n      <div style=\"flex: 76%\">\r\n        <el-carousel height=\"350px\">\r\n          <el-carousel-item v-for=\"item in carouselData\">\r\n            <img :src=\"item\" alt=\"\" style=\"width: 100%; height: 350px\">\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n      </div>\r\n      <div style=\"flex: 12%; height: 350px; background-color: #0c0c0c\"></div>\r\n    </div>\r\n    <div style=\"width: 76%; margin: 50px auto; min-height: 1000px\">\r\n      <div style=\"display: flex\">\r\n        <div style=\"flex: 1\">\r\n          <span style=\"font-size: 24px; color: #333333; margin-right: 50px\">在线课程</span>\r\n          <el-button type=\"primary\" @click=\"initValue('VIDEO')\">视频课程</el-button>\r\n          <el-button type=\"warning\" @click=\"initValue('TEXT')\">图文课程</el-button>\r\n        </div>\r\n        <div style=\"width: 300px\">\r\n          <el-button type=\"info\" @click=\"signin\">签到</el-button>\r\n          <span style=\"margin-left: 30px; color: #12b127; font-weight: 550\">上次签到：</span>\r\n          <span style=\"color: #666666\">{{ signInData?.time }}</span>\r\n        </div>\r\n      </div>\r\n      <div style=\"display: flex; margin-top: 20px; height: 300px\">\r\n        <div style=\"flex: 1; margin-right: 10px; width: 0\">\r\n          <img :src=\"recommend.img\" alt=\"\" style=\"width: 100%; height: 270px; border-radius: 5px; cursor: pointer\" @click=\"navTo(recommend.id)\">\r\n          <div style=\"font-size: 15px; margin-top: 5px\" class=\"overflowShow\">{{ recommend.name }}</div>\r\n        </div>\r\n        <div style=\"flex: 2; margin-left: 10px\">\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"6\" style=\"margin-bottom: 35px\" v-for=\"item in rightData\">\r\n              <img :src=\"item.img\" alt=\"\" style=\"width: 100%; height: 100px; border-radius: 5px; border: 1px solid #cccccc; cursor: pointer\" @click=\"navTo(item.id)\">\r\n              <div style=\"color: #333333; margin-top: 10px\" class=\"overflowShow\">{{ item.name }}</div>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n      </div>\r\n\r\n      <div style=\"margin-top: 50px\">\r\n        <span style=\"font-size: 24px; color: #333333; margin-right: 50px\">在线资源</span>\r\n      </div>\r\n      <div style=\"display: flex; margin-top: 20px; height: 300px\">\r\n        <div style=\"flex: 2; margin-right: 10px\">\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"6\" style=\"margin-bottom: 35px\" v-for=\"item in leftData\">\r\n              <img :src=\"item.img\" alt=\"\" style=\"width: 100%; height: 100px; border-radius: 5px; border: 1px solid #cccccc; cursor: pointer\" @click=\"navToInformation(item.id)\">\r\n              <div style=\"color: #333333; margin-top: 10px\" class=\"overflowShow\">{{ item.name }}</div>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n        <div style=\"flex: 1; margin-left: 10px; width: 0\">\r\n          <img :src=\"fileRecommend?.img\" alt=\"\" style=\"width: 100%; height: 270px; border-radius: 5px; cursor: pointer\" @click=\"navToInformation(fileRecommend.id)\">\r\n          <div style=\"font-size: 15px; margin-top: 5px\" class=\"overflowShow\">{{ fileRecommend?.name }}</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      carouselData: [\r\n        require('@/assets/imgs/lun-1.jpg'),\r\n        require('@/assets/imgs/lun-2.jpg'),\r\n        require('@/assets/imgs/lun-3.jpg'),\r\n        require('@/assets/imgs/lun-4.jpg'),\r\n      ],\r\n      type:'VIDEO',\r\n      recommend:{},\r\n      rightData: [],\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getData()\r\n    this.getInformation()\r\n    this.getSign()\r\n  },\r\n  methods:{\r\n    initValue(type) {\r\n      this.type = type\r\n      this.getData()\r\n      this.getInformation()\r\n    },\r\n    getData() {\r\n      if ('VIDEO' === this.type) {\r\n        // 1. 获取推荐的那个积分课程\r\n       this.$request.get('/course/getRecommend?type=VIDEO').then(res=>{\r\n         if(res.code ==='200'){\r\n           this.recommend=res.data\r\n         }else {\r\n           this.$message.error(res.msg)\r\n         }\r\n       })\r\n        // 2. 获取推荐之外的最新的8个课程\r\n      this.$request.get('/course/selectTop8?type=VIDEO').then(res=>{\r\n        if(res.code ==='200'){\r\n          this.rightData=res.data\r\n        }else {\r\n          this.$message.error(res.msg)\r\n          }\r\n        })\r\n      }\r\n      if ('TEXT' === this.type) {\r\n        // 1. 获取推荐的那个积分课程\r\n        this.$request.get('/course/getRecommend?type=TEXT').then(res=>{\r\n          if(res.code ==='200'){\r\n            this.recommend=res.data\r\n          }else {\r\n            this.$message.error(res.msg)\r\n          }\r\n        })\r\n        // 2. 获取推荐之外的最新的8个课程\r\n        this.$request.get('/course/selectTop8?type=TEXT').then(res=>{\r\n          if(res.code ==='200'){\r\n            this.rightData=res.data\r\n          }else {\r\n            this.$message.error(res.msg)\r\n          }\r\n        })\r\n      }\r\n    },\r\n\r\n    getInformation() {\r\n      // 1. 获取推荐的那个资料\r\n      this.$request.get('/information/getRecommend').then(res => {\r\n        if (res.code === '200') {\r\n          this.fileRecommend = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n      // 2. 获取非推荐的8个资料\r\n      this.$request.get('/information/selectTop8').then(res => {\r\n        if (res.code === '200') {\r\n          this.leftData = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    navTo(id) {\r\n      // 往课程详情页跳\r\n      location.href = '/front/courseDetail?id=' + id\r\n\r\n    },\r\n    navToInformation(id) {\r\n      location.href = '/front/informationDetail?id=' + id\r\n    },\r\n\r\n    getSign() {\r\n      this.$request.get('/signin/selectByUserId?id=' + this.user.id).then(res => {\r\n        if (res.code === '200') {\r\n          // 渲染数据到页面\r\n          this.signin = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    signin() {\r\n      let data = {\r\n        userId: this.user.id\r\n      }\r\n      this.$request.post('/signin/add', data).then(res => {\r\n        if (res.code === '200') {\r\n          this.getSign()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>"],"mappings":"AAgEA;EAEAA,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,YAAA,GACAC,OAAA,6BACAA,OAAA,6BACAA,OAAA,6BACAA,OAAA,4BACA;MACAC,IAAA;MACAC,SAAA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;IACA,KAAAC,cAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAC,UAAAR,IAAA;MACA,KAAAA,IAAA,GAAAA,IAAA;MACA,KAAAI,OAAA;MACA,KAAAC,cAAA;IACA;IACAD,QAAA;MACA,qBAAAJ,IAAA;QACA;QACA,KAAAS,QAAA,CAAAC,GAAA,oCAAAC,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAZ,SAAA,GAAAW,GAAA,CAAApB,IAAA;UACA;YACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;QACA;QACA,KAAAP,QAAA,CAAAC,GAAA,kCAAAC,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAX,SAAA,GAAAU,GAAA,CAAApB,IAAA;UACA;YACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;MACA;MACA,oBAAAhB,IAAA;QACA;QACA,KAAAS,QAAA,CAAAC,GAAA,mCAAAC,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAZ,SAAA,GAAAW,GAAA,CAAApB,IAAA;UACA;YACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;QACA;QACA,KAAAP,QAAA,CAAAC,GAAA,iCAAAC,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAX,SAAA,GAAAU,GAAA,CAAApB,IAAA;UACA;YACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;MACA;IACA;IAEAX,eAAA;MACA;MACA,KAAAI,QAAA,CAAAC,GAAA,8BAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAI,aAAA,GAAAL,GAAA,CAAApB,IAAA;QACA;UACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;MACA;MACA,KAAAP,QAAA,CAAAC,GAAA,4BAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAK,QAAA,GAAAN,GAAA,CAAApB,IAAA;QACA;UACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IAEAG,MAAAC,EAAA;MACA;MACAC,QAAA,CAAAC,IAAA,+BAAAF,EAAA;IAEA;IACAG,iBAAAH,EAAA;MACAC,QAAA,CAAAC,IAAA,oCAAAF,EAAA;IACA;IAEAd,QAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,qCAAAjB,IA